		"metal-events": "^2.13.2",
		"metal-soy": "^2.13.2",
		"metal-state": "^2.13.2",
		"metal-uri": "^3.1.0"
	},
	"devDependencies": {
		"check-source-formatting": "^2.0.4",
		"liferay-module-config-generator": "^1.1.10",
