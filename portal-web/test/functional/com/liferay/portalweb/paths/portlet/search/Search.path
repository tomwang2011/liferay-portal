<html>
<head>
<title>Search</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">Search</td></tr>
</thead>
<tbody>
<tr>
	<td>CLASSIC_SEARCH_BAR</td>
	<td>//input[contains(@id,'templateSearch') and @type='text']</td>
	<td></td>
</tr>
<tr>
	<td>EMBEDDED_SEARCH_BAR</td>
	<td>//header[@id='banner']//input[contains(@data-qa-id,'searchInput')]</td>
	<td></td>
</tr>
<tr>
	<td>EMBEDDED_SEARCH_BAR_ELLIPSIS_ICON</td>
	<td>//header[@class='portlet-topper' and contains(.,'Search')][ancestor::header[@id='banner']]//div[contains(@class,'portlet-options')]/a</td>
	<td></td>
</tr>
<tr>
	<td>EMBEDDED_SEARCH_BAR_SCOPE_SELECT</td>
	<td>//header[@id='banner']//div/select[@title='Scope']</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_BAR_WIDGET</td>
	<td>//input[not(ancestor::header[@id='banner'])][contains(@data-qa-id,'searchInput')]</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_BAR_WIDGET_ELLIPSIS_ICON</td>
	<td>//header[@class='portlet-topper' and contains(.,'Search Bar')][not(ancestor::header[@id='banner'])]//div[contains(@class,'portlet-options')]/a</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_BAR_WIDGET_SCOPE_SELECT</td>
	<td>//div[not(ancestor::header[@id='banner'])]/select[@title='Scope']</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_INFO</td>
	<td>//span[contains(@class,'text-truncate')]/span</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_PORTLET_OPTIONS_WRENCH</td>
	<td>//div[contains(@class,'portlet-options')]/a[contains(@id,'_SearchPortlet') and @title='Options']</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>