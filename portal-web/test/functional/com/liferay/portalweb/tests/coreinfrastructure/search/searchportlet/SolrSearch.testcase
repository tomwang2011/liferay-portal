	<property name="portal.suite.search.engine" value="solr" />
	<property name="portal.upstream" value="true" />
	<property name="solr.enabled" value="true" />
	<property name="testray.main.component.name" value="Web Search" />

	<set-up>
		<execute macro="TestCase#setUpPortalInstance" />

		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="PortalInstances#tearDownCP" />
			</then>
			<else>
				<execute macro="BlogsEntry#tearDownCP" />

				<execute macro="MessageboardsThread#tearDownCP" />

				<execute macro="User#tearDownCP" />

				<execute macro="WebContent#tearDownCP" />

				<execute macro="DMDocument#tearDownCP" />

				<execute macro="WikiPage#tearDownCP" />

				<execute macro="Vocabulary#tearDownCP" />

				<execute macro="Tag#tearDownCP" />

				<execute macro="Page#tearDownCP" />
			</else>
		</if>
	</tear-down>

	<command name="NavigateToSearchResultsViaURL" priority="4">
		<execute macro="ProductMenu#gotoPortlet">
			<var name="category" value="Content" />
			<var name="panel" value="Site Administration" />
			<var name="portlet" value="Blogs" />
		</execute>

		<execute macro="Blogs#addEntry">
			<var name="entryContent" value="This is a blog about Potatoes" />
			<var name="entryTitle" value="Potato Blog" />
		</execute>

		<execute macro="ProductMenu#gotoPortlet">
			<var name="category" value="Build" />
			<var name="panel" value="Site Administration" />
			<var name="portlet" value="Pages" />
		</execute>

		<execute macro="SitePages#addPublicPage">
			<var name="pageName" value="Search Page" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Search Page" />
		</execute>

		<execute macro="SearchPortlets#addWidgets">
			<var name="searchPortletList" value="Type Facet,Search Results,Search Bar" />
		</execute>

		<execute macro="Portlet#dragAndDropPortletToColumnPG">
			<var name="columnNumberFrom" value="1" />
			<var name="columnNumberTo" value="2" />
			<var name="portletName" value="Search Results" />
		</execute>

		<execute macro="SearchPortlets#configureWidgetParameterNames">
			<var name="keywordsParameterName" value="keywordparameter" />
			<var name="scopeParameterName" value="scopeparameter" />
		</execute>

		<execute macro="SearchPortlets#configureWidgetScope">
			<var name="searchScope" value="Let the User Choose" />
		</execute>

		<execute macro="SearchPortlets#selectWidgetScopeInput">
			<var name="searchScope" value="Everything" />
		</execute>

		<execute macro="SearchPortlets#searchWidget">
			<var name="searchTerm" value="Potato" />
		</execute>

		<execute macro="SearchFacetPortlet#checkFacet">
			<var name="facetValueName" value="Blogs Entry" />
		</execute>

		<var method="TestPropsUtil#get('portal.url')" name="portalURL" />

		<execute function="AssertLocation" value1="${portalURL}/web/guest/search-page?keywordparameter=Potato&amp;scopeparameter=everything&amp;type=com.liferay.blogs.model.BlogsEntry" />

		<var method="selenium#getCurrentUrl()" name="searchURL" />

		<execute macro="Navigator#openURL" />

		<execute macro="Navigator#openSpecificURL">
			<var name="url" value="${searchURL}" />
		</execute>

		<execute function="AssertTextEquals" locator1="Search#SEARCH_BAR_WIDGET" value1="Potato" />

		<execute macro="SearchResultPortlet#viewSearchResults">
			<var name="searchAssetTitle" value="Potato Blog" />
			<var name="searchAssetType" value="Blogs Entry" />
		</execute>

		<execute function="AssertChecked" locator1="Checkbox#ANY_CHECKBOX">
			<var name="checkboxName" value="Blogs Entry" />
		</execute>
	</command>

