<definition>
	<command name="click">
		<execute function="Click" locator1="Button#ANY">
			<var name="key_text" value="${button}" />
		</execute>
	</command>

	<command name="clickAdd">
		<execute function="AssertClick" locator1="Button#ADD" value1="Add" />
	</command>

	<command name="clickAddRow">
		<execute function="Click" locator1="Button#ADD_ROW" />
	</command>

	<command name="clickApply">
		<execute function="AssertClick" locator1="Button#APPLY" value1="Apply" />
	</command>

	<command name="clickCancel">
		<execute function="AssertClick" locator1="Button#CANCEL" value1="Cancel" />
	</command>

	<command name="clickConfirm">
		<execute function="AssertClick" locator1="Button#CONFIRM" value1="Confirm" />
	</command>

	<command name="clickDelete">
		<execute function="AssertClick" locator1="Button#DELETE" value1="Delete" />
	</command>

	<command name="clickDone">
		<execute function="AssertClick" locator1="Button#DONE" value1="Done" />
	</command>

	<command name="clickInstall">
		<execute function="AssertClick" locator1="Button#SUBMIT" value1="Install" />
	</command>

	<command name="clickNext">
		<execute function="AssertClick" locator1="Button#NEXT" value1="Next" />
	</command>

	<command name="clickNoError">
		<execute function="ClickNoError" locator1="Button#ANY">
			<var name="key_text" value="${button}" />
		</execute>
	</command>

	<command name="clickPublish">
		<execute function="ScrollWebElementIntoView" locator1="Button#ANY">
			<var name="key_text" value="Publish" />
		</execute>

		<execute macro="Button#click">
			<var name="button" value="Publish" />
		</execute>
	</command>

	<command name="clickRemove">
		<execute function="AssertClick" locator1="Button#REMOVE" value1="Remove" />
	</command>

	<command name="clickSave">
		<execute function="AssertClick" locator1="Button#SAVE" value1="Save" />
	</command>

	<command name="clickSaveAsDraft">
		<execute function="AssertClick" locator1="Button#SAVE_AS_DRAFT" value1="Save as Draft" />
	</command>

	<command name="clickSubmitForPublication">
		<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />
	</command>

	<command name="clickSubmitLocalized">
		<execute function="AssertClick" locator1="Button#SUBMIT" value1="${buttonName}" />
	</command>

	<command name="clickUpdate">
		<execute function="AssertClick" locator1="Button#UPDATE" value1="Update" />
	</command>

	<command name="viewNotPresent">
		<execute function="AssertElementNotPresent" locator1="Button#ANY">
			<var name="key_text" value="${button}" />
		</execute>
	</command>

	<command name="viewPresent">
		<execute function="AssertElementPresent" locator1="Button#ANY">
			<var name="key_text" value="${button}" />
		</execute>
	</command>
</definition>